package javaimplementation;

public class practiceforjob2 {

	public static int fibbo(int n) {
		if(n==0||n==1) {
			return n;
		}
		int fn1=fibbo(n-1);
		int fn2=fibbo(n-2);
		int fn =fn1+fn2;
		return fn;
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
System.out.println(fibbo(5));
	}

}
