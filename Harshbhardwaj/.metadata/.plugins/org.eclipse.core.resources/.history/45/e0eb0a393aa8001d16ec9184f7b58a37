package merryjane;

import java.util.Scanner;

public class string1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
Scanner sc=new Scanner(System.in);
String name;
String line;
name=sc.next();
System.out.println(name);
line=sc.nextLine();
System.out.println(line);
	}

}
