package merryjane;

public class binsrch {
	public static int binsearch(int arr[], int key) {
		int start = 0;
		int end = arr.length - 1;
		while (start <= end) {
			int mid = (start + end) / 2;

			if (arr[mid] == key) {
				//System.out.println("found")
				return mid;
			} else if (arr[mid] < key) {
				start = mid + 1;

			} else if (arr[mid] > key) {
				end = mid - 1;
			}
		}
		return -1;
	}

	public static void main(String[] args) {
		int arr1[] = { 15, 22, 34, 48, 65 };
		int key=22;
		binsearch(arr1,key );

	}

}
