package merryjane;

public class bubblesort {
public static void bubblesrt(int arr[]) {
	for(nt i=0;i<=arr.length-2;i++) {
		for(int j=0;j<=arr.length-i;j++) {
			if(arr[j]>arr[i]) {
				int temp;
				temp=arr[j];
				arr[j]=arr[i];
				arr[i]=temp;
			}
			else {
				continue;
			}
		}
	}
	for(int k=0;k<arr.length;k++) {
		System.out.print(arr[k]);
	}
}
	public static void main(String[] args) {
int arr1[]= {5,2,1,3,2};
bubblesrt(arr1);

	}

}
