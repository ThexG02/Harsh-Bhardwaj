package merryjane;

import java.util.Scanner;

public class precfo {
	public static void agekey(int key[]) {
		for (int i = 0; i < key.length; i++) {
			key[i] = (key[i] * 10 + 5);
		}

	}
	public static void lsearch(int arr[],int key) {
		for(int i=0;i<arr.length;i++) {
			if(arr[i]==key) {
				System.out.println("found");
				break;
			}
			else {
			 continue;
			}
		}
	}
	
	public static void lsstr(String arr[],String key) {
		for(int i=0;i<arr.length;i++) {
			if(arr[i].equals(key)) {
				System.out.println("found");
				break;
			}
			else {
				continue;
			}
		}
	}
	public static int bsrch(int arr[],int key) {
		int s=0;
		int e=arr.length;
		int mid=(s+e)/2;
		for(int i=0;i<arr.length;i++) {
			if(key==arr[mid]) {
				return mid;
			}
			else if(key>arr[mid]) {
				s=mid+1;
			}
			else {
				e=mid-1;
			}
		}
		return -1;
	}
	public static void main(String[] args) {

		int age[] = { 12, 28, 31, 24, 15, 96, 733, 28, 59 };
		int arr1[]= {12,34,56,78,90};
		String name[]= {"harsh","bhardwaj","sahil","pahwa","aastha","arora"};
		//agekey(age);
		lsearch(age,96);
		lsstr(name,"bhardwaj");
		System.out.println(bsrch(arr1,78));
		
		}

	}
