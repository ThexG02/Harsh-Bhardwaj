package merryjane;

import java.util.Scanner;

public class precfo {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int n =  sc.nextInt();
		for(int i=0;i<n;i++) {
			//sPACES 
			for(int j=n;j>i;j--) {
				System.out.print(" ");
			}
			//stars 
			for(int j=i;j<n;j++) {
				
				System.out.print("*");}
				
			}
			
			
			System.out.println();
		}


		
	

	
}

